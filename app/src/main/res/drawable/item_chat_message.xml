<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 테두리 내부 색상 설정 -->
    <solid android:color="#ffffff" />

    <!-- 테두리 내부 여백 설정 -->
    <padding
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        />

    <corners
        android:radius="5dp"
        />

</shape>
